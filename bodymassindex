#include<stdio.h>
void main()
{
  float weight,height,bmi;
  printf("Enter weight:");
  scanf("%f",&weight);
  printf("Enter height:");
  scanf("%f",&height);
  bmi=weight/(height*height);
  printf("your BMI is :%.2f\n",bmi);
  if(bmi<18.5){
      printf("under weight");
  }
  else if (bmi<24.9)
  {
      printf("normal weight");
  }
  else if (bmi<29.9)
  {
      printf("over weight");
  }
  else
  {
      printf("obese");
  }
}
